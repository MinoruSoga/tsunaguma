{
  "name": "backend-ec",
  "version": "0.0.1",
  "description": "Backend EC for tng",
  "author": "Sebastian Rindom <skrindom@gmail.com>",
  "license": "MIT",
  "scripts": {
    "test": "jest",
    "seed": "DEBUG=* NODE_ENV=development node ./dist/commands/seed.js",
    "build": "rm -rf dist && ./node_modules/.bin/tsc -p tsconfig.build.json",
    "start": "npm run build && NODE_ENV=development node ./dist/main.js",
    "start:watch": "nodemon --watch './src' --watch './medusa-config.js' --ignore '**/*.test.ts' --ignore '**/*.test.js' --ignore '**/*.spec.js' --ignore '**/*.spec.ts' --ext 'js,json,ts,tsx' --exec 'npm run build && NODE_ENV=development node ./dist/main.js  --max_old_space_size=8048'",
    "start:prod": "NODE_ENV=production node dist/main",
    "lint": "eslint --ignore-path .eslintignore --ext .js,.ts,.tsx .",
    "email:dev": "email dev",
    "prettier": "prettier",
    "openapi:sdk:admin": "openapi -i ./client-sdk/medusa/docs/admin-spec3.json -o ./client-sdk/medusa/admin -c axios --useOptions --exportSchemas true --indent 2 --request ./client-sdk/request/index.ts",
    "openapi:sdk:store": "openapi -i ./client-sdk/medusa/docs/store-spec3.json -o ./client-sdk/medusa/store -c axios --useOptions --exportSchemas true --indent 2 --request ./client-sdk/request/index.ts",
    "openapi:generate": "node ./scripts/build-openapi.js && npm run openapi:sdk:admin && npm run openapi:sdk:store"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.216.0",
    "@aws-sdk/client-ses": "^3.216.0",
    "@aws-sdk/s3-request-presigner": "^3.216.0",
